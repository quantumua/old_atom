<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="login-sequential" verbose="1">
    <test name="Login tests - sequential execution of tests">
        <classes>
            <class name="com.betamedia.atom.testslibrary.option24.end2end.LoginDialogTest" >
                <methods>
                    <parameter name="username" value="maksym"/>
                    <parameter name="password" value="password"/>
                    <include name="bidDuringZeroBalanceTest"/>
                    <include name="bidMoreThanNoneZeroBalanceTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
