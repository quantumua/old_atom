<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite: sequential execution of tests" verbose="1">
    <test name="Tryouts 1">
        <classes>
            <class name="com.betamedia.qe.af.testslibrary.option24.end2end.crm.RejectedUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore0IsRejectedTest"/>
                    <include name = "checkCustomerWithScore10IsRejectedTest"/>
                    <include name = "checkCustomerWithScore15IsRejectedTest"/>
                    <include name = "checkRejectedCustomerAccessNoLoginTest"/>
                </methods>
            </class>
            <class name="com.betamedia.qe.af.testslibrary.option24.end2end.crm.NoExperienceUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore16IsNoExperienceTest"/>
                    <include name = "checkCustomerWithScore25IsNoExperienceTest"/>
                    <include name = "checkCustomerWithScore30IsNoExperienceTest"/>
                    <include name = "checkNoExperienceCustomerAccessActiveTest"/>
                </methods>
            </class>
            <class name="com.betamedia.qe.af.testslibrary.option24.end2end.crm.HighExperienceUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore51IsHighExperienceTest"/>
                    <include name = "checkCustomerWithScore65IsHighExperienceTest"/>
                    <include name = "checkCustomerWithScore70IsHighExperienceTest"/>
                    <include name = "checkHighExperienceCustomerAccessActiveTest"/>
                </methods>
            </class>
            <class name="com.betamedia.qe.af.testslibrary.option24.end2end.crm.ExpertUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore71IsExpertTest"/>
                    <include name = "checkCustomerWithScore85IsExpertTest"/>
                    <include name = "checkCustomerWithScore100IsExpertTest"/>
                    <include name = "checkExpertCustomerAccessActiveTest"/>
                </methods>
            </class>
            <class name="com.betamedia.qe.af.testslibrary.option24.end2end.crm.UnknownExperienceUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore16IsUnknownExperienceTest"/>
                </methods>
            </class>

            <class name="com.betamedia.qe.af.testslibrary.option24.end2end.crm.WebsiteUserExperienceTest">
                <methods>
                    <include name = "score31CustomerExperienceTest"/>
                    <include name = "score45CustomerExperienceTest"/>
                    <include name = "score50CustomerExperienceTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
