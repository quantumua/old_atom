<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="All levels experience of user tests" verbose="1" parallel="methods" thread-count="4">
    <test name="All levels experience of user tests">
        <classes>
            <class name="com.betamedia.atom.testslibrary.option24.end2end.crm.experienceLevels.RejectedUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore0IsRejectedTest"/>
                    <include name = "checkCustomerWithScore10IsRejectedTest"/>
                    <include name = "checkCustomerWithScore15IsRejectedTest"/>
                    <include name = "checkRejectedCustomerAccessNoLoginTest"/>
                </methods>
            </class>
            <class name="com.betamedia.atom.testslibrary.option24.end2end.crm.experienceLevels.NoExperienceUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore16IsNoExperienceTest"/>
                    <include name = "checkCustomerWithScore25IsNoExperienceTest"/>
                    <include name = "checkCustomerWithScore30IsNoExperienceTest"/>
                    <include name = "checkNoExperienceCustomerAccessActiveTest"/>
                </methods>
            </class>
            <class name="com.betamedia.atom.testslibrary.option24.end2end.crm.experienceLevels.HighExperienceUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore51IsHighExperienceTest"/>
                    <include name = "checkCustomerWithScore65IsHighExperienceTest"/>
                    <include name = "checkCustomerWithScore70IsHighExperienceTest"/>
                    <include name = "checkHighExperienceCustomerAccessActiveTest"/>
                </methods>
            </class>
            <class name="com.betamedia.atom.testslibrary.option24.end2end.crm.experienceLevels.ExpertUserTest" >
                <methods>
                    <include name = "checkCustomerWithScore71IsExpertTest"/>
                    <include name = "checkCustomerWithScore85IsExpertTest"/>
                    <include name = "checkCustomerWithScore100IsExpertTest"/>
                    <include name = "checkExpertCustomerAccessActiveTest"/>
                </methods>
            </class>
            <class name="com.betamedia.atom.testslibrary.option24.end2end.crm.experienceLevels.UnknownExperienceUserTest" >
                <methods>
                    <include name = "checkCustomerWithNoScoreIsUnknownExperienceTest"/>
                </methods>
            </class>

            <class name="com.betamedia.atom.testslibrary.option24.end2end.crm.experienceLevels.LowExperienceUserTest">
                <methods>
                    <include name = "checkCustomerWithScore31IsLowExperienceTest"/>
                    <include name = "checkCustomerWithScore45IsLowExperienceTest"/>
                    <include name = "checkCustomerWithScore50IsLowExperienceTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
